{
  "version": 2,
  "builds": [
    {
      "src": "api/backend/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/backend/(.*)",
      "dest": "/api/backend/index.py"
    },
    {
      "src": "/api/backend/(.*)",
      "dest": "/api/backend/index.py"
    }
  ]
}
