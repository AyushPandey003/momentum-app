{
  "rewrites": [
    {
      "source": "/backend/:path*",
      "destination": "/api/backend/index.py"
    }
  ],
  "headers": [
    {
      "source": "/api/auth/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ]
}