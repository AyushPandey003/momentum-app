{
  "version": 2,
  "buildCommand": "next build",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "backend/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/backend/(.*)",
      "dest": "backend/index.py"
    }
  ]
}
